{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Electron",
            "type": "node",
            "program": "build/main.js",
            "stopOnEntry": false,
            "args": [], 
            "cwd": ".",
            "runtimeExecutable": "node_modules/electron/dist/electron.exe",
            
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"sourceMaps": false,
			"outDir": null,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	]
}